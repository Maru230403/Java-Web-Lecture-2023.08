!-- kakaoMap.html -->

<!DOCTYPE html>
<html>
<head>
    <title>KakaoMap Example</title>
    <script type="text/javascript" src="https://dapi.kakao.com/v2/maps/sdk.js?appkey=ec9fb865ecc96649854fe4493916d802"></script>
    <style>
        #map {
            width: 100%;
            height: 500px;
        }
    </style>
</head>
<body>

<div id="map"></div>

<script>
    var container = document.getElementById('map');
    var options = {
        center: new kakao.maps.LatLng(37.5665, 126.9780), // 서울의 좌표를 설정합니다.
        level: 5 // 지도의 확대 레벨을 설정합니다.
    };
    
    var map = new kakao.maps.Map(container, options); // 지도를 생성합니다.
</script>

</body>
</html>
